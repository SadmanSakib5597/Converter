<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <AnchorPane opacity="0.55" prefHeight="230.0" prefWidth="330.0" style="-fx-background-color: blue;">
         <children>
            <Button fx:id="Select_file" layoutX="42.0" layoutY="47.0" mnemonicParsing="false" onAction="#getpath" text="select File..." />
            <Label fx:id="path" layoutX="164.0" layoutY="44.0" prefHeight="32.0" prefWidth="128.0" style="-fx-background-color: white;" text="      path.... ... ....." textFill="#0e8439" />
            <ComboBox fx:id="choose_file" layoutX="89.0" layoutY="95.0" prefWidth="150.0" promptText="Choose File Type" style="-fx-background-color: white;" />
            <Button fx:id="convert_file" layoutX="136.0" layoutY="134.0" mnemonicParsing="false" onAction="#file_convert" text="Convert" />
            <Button layoutX="146.0" layoutY="176.0" mnemonicParsing="false" onAction="#exitButton" text="Exit" textFill="#065431" />
         </children>
      </AnchorPane>
   </children>
</GridPane>
